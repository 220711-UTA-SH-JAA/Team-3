<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://JAVA.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>EcommerceApp</display-name>
  <welcome-file-list>
    <welcome-file>resources/HTML/index.html</welcome-file>
  </welcome-file-list>
  
  <servlet>
  	<servlet-name>UserFrontController1</servlet-name>
  	<servlet-class>com.example.servlets.UserFrontController1</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>UserFrontController1</servlet-name>
  	<url-pattern>/API/*</url-pattern>
  </servlet-mapping>
  
</web-app>